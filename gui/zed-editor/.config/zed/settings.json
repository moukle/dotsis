{
  "git_panel": {
    "sort_by_path": true
  },
  "icon_theme": "Material Icon Theme",
  "show_edit_predictions": false,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "languages": {
    "Julia": {
      "show_edit_predictions": false
    }
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-3.5-turbo"
    }
  },
  "project_panel": {
    "default_width": 150,
    "dock": "right"
  },
  "base_keymap": "VSCode",
  "theme": {
    "mode": "system",
    "light": "Gruber Pear Lite üçê",
    "dark": "Phine"
  },
  "relative_line_numbers": true,
  "vim_mode": true,
  "ui_font_size": 22,
  "ui_font_family": "Maple Mono NF",
  "buffer_font_family": "Maple Mono NF",
  "buffer_font_size": 20,
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "ss02": true,
    "ss19": false,
    "cv18": true,
    "cv14": true
  },
  "git": {
    "inline_blame": { "enabled": false }
  },
  "wrap_guides": [80, 120]
}
