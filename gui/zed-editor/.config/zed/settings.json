{
  "languages": {
    "Julia": {
      "show_inline_completions": false
    }
  },
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-3.5-turbo"
    },
    "version": "2"
  },
  "project_panel": {
    "default_width": 150,
    "dock": "right"
  },
  "base_keymap": "VSCode",
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Phine"
  },
  "relative_line_numbers": true,
  "vim_mode": true,
  "ui_font_size": 20,
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 20,
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "ss02": true,
    "ss19": false,
    "cv18": true,
    "cv14": true
    // "cv01": true,
    // "ss01": true,
    // "ss02": true,
    // "ss04": true,
    // "ss05": true
  },
  "git": {
    "inline_blame": { "enabled": false }
  },
  "wrap_guides": [80, 120]
}
